/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(a=sel,out=c);
    Nand(a=c,b=a,out = d);
    Nand(a=sel,b=b,out=e);
    Or(a=d,b=e,out = out);
}