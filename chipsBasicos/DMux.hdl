/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:

    Or(a=in, b=sel, out=i1);
    Not(in=i1, out=i2);
    Not(in=sel, out=i3);
    Xor(a=i2, b=i3, out=a); 
    And(a=in, b= sel, out=b) ;  
}
