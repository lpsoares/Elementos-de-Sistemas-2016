// File name: Xnor.hdl

/**
 * Exclusive-nor gate:
 * out = (a == b)
 */

CHIP Xnor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nandab);
    Nand(a=a, b=nandab, out=i1);
    Nand(a=nandab, b=b, out=i2);
    Nand(a=i1, b=i2, out=axorb);
    Nand(a=axorb, b=axorb, out=out);
}