/**
 * Exclusive-nor gate:
 * out = (a == b)
 */

CHIP Xnor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=a,b=b,out=c);
    Nand(a=a,b=c,out=d);
    Nand(a=b,b=c,out=e);
    Nand(a=d,b=e,out=f);
    Nand(a=f,b=f,out=out);
}