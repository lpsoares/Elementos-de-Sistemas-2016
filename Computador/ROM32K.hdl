CHIP Memory {
    IN address[15];
    OUT out[16];

    PARTS:
    // Put your code here:
    RAM16K(in=false,load=false,address=address[1..15],out=outRAM0)
    RAM16K(in=false,load=false,address=address[1..15],out=outRAM1)
    Mux16(a=outRAM0,b=outRAM1,sel=address[0],out=out)
}